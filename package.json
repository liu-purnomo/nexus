{
  "name": "nexus-orm",
  "version": "0.1.0",
  "description": "Type-safe ORM for Next.js & PostgreSQL with intuitive developer experience",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "example:basic": "ts-node examples/basic-usage.ts",
    "example:migration": "ts-node examples/migration-example.ts",
    "setup:db": "node scripts/setup-db.js"
  },
  "keywords": [
    "orm",
    "typescript",
    "postgresql",
    "nextjs",
    "database",
    "type-safe",
    "query-builder"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "pg": "^8.11.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/nexus-orm.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/nexus-orm/issues"
  },
  "homepage": "https://github.com/your-username/nexus-orm#readme"
}
